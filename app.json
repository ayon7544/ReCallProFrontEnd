{
  "expo": {
    "name": "Recall Pro",
    "slug": "recall-pro",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "recallpro",
    "userInterfaceStyle": "automatic",
    "icon": "./assets/images/AppIcon.png",
    "statusBar": {
      "barStyle": "dark-content",
      "backgroundColor": "transparent",
      "translucent": true
    },
    "splash": {
      "image": "./assets/images/AppIcon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0F0B18"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.ayonghosh.recallpro",
      "adaptiveIcon": {
        "backgroundColor": "#0F0B18"
      },
      "navigationBar": {
        "visible": false,
        "barStyle": "light-content",
        "backgroundColor": "#00000000"
      },
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "bundler": "metro"
    }
  }
}